{
    "presets": [
        "env",
        "react",
        "stage-2"
    ],
    "plugins": [
        "transform-object-rest-spread",
		["module-resolver", {
            "root": ["./src"],
            "alias": {
                "utils": "./src/utils",
                "theme": "./src/theme",
                "atoms": "./src/atoms",
                "molecules": "./src/molecules",
                "organisms": "./src/organisms",
                "styles": "./src/styles"
            }
        }]
    ]
}
